include ../room-card/room-card.pug
include ../small.blocks/pagination/pagination.pug
mixin cards-for-rooms()
    div.cards-for-rooms
        h1 Номера, которые мы для вас подобрали
        div.cards-container
            - for(var i = 1; i < 26; i++)
                +room-card("room_"+i, i+1)
        +pagination()            